{
    "version": "1.0.2",
    "name": "od/sw6-klaviyo-integration",
    "description": "Shopware 6 plugin for Klaviyo integration",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Klaviyo\\Integration\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/*",
            "options": {
                "symlink": false
            }
        }
    ],
    "extra": {
        "shopware-plugin-class": "Klaviyo\\Integration\\KlaviyoIntegrationPlugin",
        "plugin-icon": "src/Resources/config/plugin.png",
        "label": {
            "de-DE": "Klaviyo Integration Plugin",
            "en-GB": "Klaviyo Integration Plugin"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "shopware/administration": "v6.4.*",
        "shopware/core": "v6.4.*",
        "shopware/storefront": "v6.4.*",
        "od/sw6-job-scheduler": "^1.0.0",
        "ext-json": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.9.1"
    }
}
